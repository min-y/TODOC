<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>

	<!-- 별칭 지정 -->
	<typeAliases>
		<typeAlias type="com.scsa.model.vo.Pharmacy" alias="Pharmacy"/>
		<typeAlias type="com.scsa.model.vo.Medicine" alias="Medicine"/>
		<typeAlias type="com.scsa.model.vo.PrescriptionHistory" alias="PrescriptionHistory"/>
		<typeAlias type="com.scsa.model.vo.PharmTodayList" alias="PharmTodayList"/>
		<typeAlias type="com.scsa.model.vo.PharmTimeTable" alias="PharmTimeTable"/>
		<typeAlias type="com.scsa.model.vo.Prescription" alias="Prescription"/>
		<typeAlias type="com.scsa.model.vo.PrescriptionMedicineList" alias="PrescriptionMedicineList"/>
	</typeAliases>

	<mappers>
		<mapper resource="Pharmacy.xml"/>
		<mapper resource="PharmTodayList.xml"/>
		<mapper resource="Prescription.xml"/>
		<mapper resource="PrescriptionHistory.xml"/>
		<mapper resource="PrescriptionMedicineList.xml"/>
	</mappers>
  
</configuration>